{
  "name": "jefferyjob/qiniu-laravel-storage",
  "type": "library",
  "description": "Qiniu Resource (Cloud) Storage SDK for Laravel 5/6/7/8/9/10",
  "keywords": [
    "qiniu",
    "storage",
    "sdk",
    "cloud",
    "laravel"
  ],
  "require": {
    "php": ">=8.0",
    "qiniu/php-sdk": "^7.2",
    "league/flysystem": "^3.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Jefferyjob\\QiniuStorage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jefferyjob\\QiniuStorage\\QiniuFilesystemServiceProvider"
      ]
    }
  },
  "config": {
    "minimum-stability": "beta",
    "prefer-stable": true
  },
  "license": "MIT"
}
